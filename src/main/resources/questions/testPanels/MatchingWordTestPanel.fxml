      maxWidth="1.7976931348623157E308"
      xmlns="http://javafx.com/javafx/17.0.2-ea"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:id="rootNode"
      fx:controller="TestCreator.questions.testPanels.MatchingWordTestPanel">

    <HBox fx:id="listViewContainer" alignment="CENTER">
        <ListView fx:id="keyListView" maxHeight="1.7976931348623157E308"
                  HBox.hgrow="ALWAYS" styleClass="brightListView"/>
        <ListView fx:id="valueListView" maxHeight="1.7976931348623157E308"
                  HBox.hgrow="ALWAYS" styleClass="brightListView"/>
    </HBox>
</VBox>
