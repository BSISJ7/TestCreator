<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<StackPane fx:id="rootNode" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TestCreator.users.ChangeEmail">

    <VBox alignment="CENTER" spacing="20.0">
        <Label fx:id="emailLabel" alignment="CENTER" text="Current Email: " VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin></Label>
        <HBox alignment="CENTER" spacing="10.0" VBox.vgrow="ALWAYS">
            <Label alignment="CENTER" text="New Email: " />
            <TextField fx:id="emailTextField" alignment="CENTER" promptText="Enter new email" />
        </HBox>
        <HBox alignment="BOTTOM_CENTER" spacing="20.0" VBox.vgrow="ALWAYS">
            <Button fx:id="changeEmailButton" disable="true" onAction="#changeEmail" text="Change Email" />
            <Button onAction="#returnToUserEditor" text="Return" />
         <VBox.margin>
            <Insets bottom="50.0" />
         </VBox.margin>
        </HBox>
    </VBox>
</StackPane>
