<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<DialogPane xmlns:fx="http://javafx.com/fxml" fx:id="newQuestionDialog" xmlns="http://javafx.com/javafx"
            fx:controller="com.BSISJ7.TestCreator.questions.editorPanels.NewQuestionEditor">

    <content>
        <GridPane vgap="10" hgap="10">
            <ChoiceBox fx:id="typesChoiceBox" GridPane.columnSpan="3" GridPane.fillWidth="true"
                       GridPane.rowIndex="0" GridPane.columnIndex="0" maxWidth="Infinity"/>
            <Label text="Name" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <TextField fx:id="name" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
        </GridPane>
    </content>
</DialogPane>
